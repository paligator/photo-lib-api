		{
			"name": "Login",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"id": "3fe536ab-70b1-4249-8277-56c90a5def9b",
						"exec": [
							"/*\r",
							"pm.sendRequest({\r",
							"    url: `${pm.environment.get(\"url\")}/auth/login`,\r",
							"    method: 'POST',\r",
							"    header: 'Content-Type:application/json',\r",
							"    body:{\r",
							"        mode: 'raw',\r",
							"        raw: JSON.stringify({ email: pm.environment.get(\"superAdmin_email\"), password: pm.environment.get(\"superAdmin_password\") })\r",
							"    }\r",
							"}, function (err, res) {\r",
							"    if(err) {\r",
							"        console.error(err);\r",
							"        return;\r",
							"    }\r",
							"    pm.environment.set(\"superAdmin_token\", res.json().data.token);\r",
							"});\r",
							"\r",
							"*/"
						],
						"type": "text/javascript"
					}
				},
				{
					"listen": "test",
					"script": {
						"id": "ba63d80e-2ef1-408c-8fe2-caa415c8da99",
						"exec": [
							"pm.test(\"response is ok\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"    \r",
							"    pm.environment.set(\"superAdmin_token\", pm.response.json().data.token);\r",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [
					{
						"key": "Content-Type",
						"name": "Content-Type",
						"value": "application/json",
						"type": "text"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\r\n\t\"email\": \"{{superAdmin_email}}\",\r\n\t\"password\": \"{{superAdmin_password}}\"\r\n}\r\n"
				},
				"url": {
					"raw": "{{url}}/auth/login",
					"host": [
						"{{url}}"
					],
					"path": [
						"auth",
