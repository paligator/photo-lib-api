{
	"swagger": {
		"outputDirectory": "./build2",
		"entryFile": "./src/**/*.ts",
		"outputFormat": "openapi_3",
		"host": "localhost:8888",
		"version": "1.0",
		"name": "Typescript-rest Test API",
		"description": "a description",
		"license": "MIT",
		"basePath": "/v1",
		"securityDefinitions": {
			"api_key": {
				"type": "apiKey",
				"name": "access_token",
				"in": "query"
			}
		},
		"ignore": [
			"**/node_modules/**"
		]
	}
}